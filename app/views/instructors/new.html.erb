<h1>Instructors#new</h1>

<h3>Create an Instructor</h3>
<%= form_with model: @instructor do |form| %>
  <%= form.text_field :name, placeholder: "name" %>
  <%= form.text_field :email, placeholder: "email" %>
  <%= form.submit %>
<% end %>

